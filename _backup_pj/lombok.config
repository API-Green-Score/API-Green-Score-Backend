lombok.log.fieldName=LOGGER
lombok.equalsAndHashCode.callSuper=call

lombok.fieldNameConstants.uppercase=true

# tells Lombok that this is your root directory and that it should not search parent directories
# for more configuration files
# (you can have more than one lombok config files in different directories/packages)
config.stopBubbling = true

# will add @lombok.Generated annotation to all Lombok generated methods
# After adding this line, when you go to Sonar,
# you will see that these classes are covered 100%
lombok.addLombokGeneratedAnnotation = true